import hudson.model.*
import jenkins.security.*

def config = new ConfigSlurper().parse(new File('<%= @dslconfig -%>').toURL())

User u = User.get(config.wrkspuser)
ApiTokenProperty t = u.getProperty(ApiTokenProperty.class)  
def token = t.getApiToken()
//token.getClass()
File fact_file = new File("/usr/local/home/jenkins/jenkins_token.txt")
fact_file.write "wrkspuser_token=$token"
